<div style="width: 700px; margin: 0px auto;">
    <button onClick="location.href='/users/insert'">사용자등록</button>
</div>
<table id="users"></table>
<script id="temp" type="text/x-handlebars-template">
    <tr class="title">
        <td width="100">아이디</td>
        <td width="100">이름</td>
        <td width="200">이메일</td>
        <td width="500">주소</td>
    </tr>
    {{#each .}}
    <tr class="row" onclick="location.href='/users/read?userid={{userid}}'">
        <td>{{userid}}</td>
        <td>{{username}}</td>
        <td>{{email}}</td>
        <td>{{address}}</td>
    </tr>
    {{/each}}
</script>
<script>
    $.ajax ({
        type:"get",
        url:"/users/list.json",
        dataType:"json",
        success : function(data) {
            temp = Handlebars.compile($("#temp").html());
            $("#users").html(temp(data));
        }
    })
</script>